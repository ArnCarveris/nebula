"namespace": "GraphicsFeature"
"includes": ["renderutil/mayacamerautil.h"]
"attributes": {
    "graphicsEntity": {
        "type": "uint"
        "fourcc": "gEnt"
        "access": "R"
		"default": -1
    }
	"modelResource": {
        "type": "string"
        "fourcc": "mdlR"
        "access": "RW"
		"default": "mdl:Buildings/castle_tower.n3"
    }
    "cameraMode": {
        "type": "uint"
        "fourcc": "cMod"
        "access": "RW"
        "default": 0
    }    
    "cameraMoveSpeed": {
        "type": "float"
        "fourcc": "cmsp"
        "access": "RW"
        "default": 1.0
        }
    "defaultCamera": {
        "type": "bool"
        "fourcc": "dflC"
        "access": "RW"
        "default": true
    }
    "cameraProjection": {
        "type": "float4"
        "fourcc": "camP"
        "access": "RW"
        "default": [ 1.3333  45.0 0.01 1000.0]
    }    
}
"components": {
    "GraphicsComponent": {
        "fourcc": "GFXC"
        "attributes": [
            "graphicsEntity"
			"modelResource"
        ]
        "events": [
            "OnActivate"
            "OnDeactivate"
        ]
    }
    "UtilCameraComponent": {
        "fourcc": "UCam"
        "attributes": [
        "graphicsEntity"
        "cameraMode"
        "cameraMoveSpeed"
        "defaultCamera"
        "cameraProjection"        
        ]
        "events": [
            "OnActivate"
            "OnDeactivate"
            "BeginFrame"
        ]
    }
}
